{
	"manifest_version": 2,
	"name": "caichuibin: chrome plugin viewer",
	"version": "0.0.4",
	"background": { "scripts": ["background.js"] },
	"permissions": ["tabs", "https://*.google.com/"],
	"page_action": {
		"default_icon": {
			"19": "cnblogs_19.png",
			"38": "cnblogs_38.png"
		},
		"default_title": "caichuibin: chrome plugin developing...",
        "default_popup": "popup.html"
	},
    "content_scripts":[{
		"matches":["https://www.google.com/*"],
		"js":["jquery-2.0.0.min.js", "content_script.js"],
        "run_at":"document_idle"
	}]
}
